import java.util.ArrayList;

public class Hello {
	public static void main(String[] args) {
		String a = "hello";
		String b = "world";
		System.out.println(a + b);
		ArrayList<String> list = new ArrayList<String>();
		list.add(a);
		list.add(b);
		for (String string : list) {
			System.out.println();
		}
	}

}
